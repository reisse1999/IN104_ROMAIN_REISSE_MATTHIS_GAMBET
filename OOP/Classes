class Vehicule:
    def __init__(self,poids0,age0,couleur0):
        self.poids = poids0
        self.age = age0
        self.couleur = couleur0
    x = 2

class Voiture(Vehicule):
    def caracteristiques(self,diam0,long0):
        self.diametreVolant = diam0
        self.longueurEssuiGlace = long0

class Moto(Vehicule):
    def caracteristiques(self,longu0,rev0):
        self.longueurGuidon = longu0
        self.revetementSelle = rev0

a = Vehicule(100,10,"rouge")
print(a.age)

b = Voiture(200,2,"bleu")
print(b.couleur)

b.caracteristiques(60,20)
print(b.diametreVolant)

print(a.x)
